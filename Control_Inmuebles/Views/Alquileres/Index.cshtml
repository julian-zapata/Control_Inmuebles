        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContratoDepartamentoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdificioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartamentoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorCuota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorAgua)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorRentas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorMunicipalidad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
