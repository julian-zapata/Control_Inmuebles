                @Html.DisplayNameFor(model => model.ContratoDepartamentoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdificioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DepartamentoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorCuota)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAgua)
            </th>
            <th>
