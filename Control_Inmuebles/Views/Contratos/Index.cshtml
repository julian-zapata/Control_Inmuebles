@model IEnumerable<Control_Inmuebles.Models.Vinculos.Contrato>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AltaContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlazoContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClausuraContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PropietarioId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InquilinoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InmuebleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DestinoInmueble)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GaranteId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AltaContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlazoContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClausuraContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropietarioId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InquilinoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InmuebleId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DestinoInmueble)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GaranteId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
